{
    "name": "styleflasher/sfentityexport-ls",
    "description": "The legacy part of the doctrine entity export extension for ezpublish 5",
    "type": "ezpublish-legacy-extension",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Styleflasher Team",
            "homepage": "http://www.styleflasher.at"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../entityexportbundle",
            "options": {
                "symlink": true
            }
